{
  "name": "hotel",
  "version": "0.4.5",
  "description": "Easily start, stop and access your servers from the browser",
  "main": "lib",
  "bin": "bin/index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha && standard",
    "start": "npm run babel:watch & npm run nodemon & wait",
    "prepublish": "npm test && npm run babel:build",
    "uninstall": "node lib/scripts/uninstall",
    "nodemon": "nodemon lib/daemon --ignore lib/daemon/public",
    "babel:build": "babel src --out-dir lib --copy-files --loose all",
    "babel:watch": "npm run build -- --watch",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typicode/hotel.git"
  },
  "keywords": [
    "dev",
    "utility",
    "process",
    "manager",
    "local",
    "server"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typicode/hotel/issues"
  },
  "homepage": "https://github.com/typicode/hotel",
  "dependencies": {
    "body-parser": "^1.14.2",
    "chokidar": "^1.2.0",
    "connect-sse": "^1.2.0",
    "debug": "^2.2.0",
    "express": "^4.13.3",
    "get-port": "^2.0.0",
    "http-proxy": "^1.12.0",
    "mkdirp": "^0.5.1",
    "once": "^1.3.2",
    "respawn-group": "^1.0.1",
    "server-ready": "^0.2.0",
    "strip-ansi": "^3.0.0",
    "sudo-block": "^1.2.0",
    "tildify": "^1.1.2",
    "unquote": "^1.1.0",
    "untildify": "^2.0.0",
    "update-notifier": "^0.5.0",
    "user-startup": "^0.1.1",
    "vhost": "^3.0.2",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.1.2",
    "babel-plugin-transform-object-assign": "^6.1.18",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.1.2",
    "mocha": "^2.4.2",
    "mock-fs": "^3.6.0",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.3",
    "standard": "^4.5.4",
    "supertest": "^0.15.0"
  }
}
