{
  "name": "hotel",
  "version": "0.4.8",
  "description": "Easily start, stop and access your servers from the browser",
  "main": "lib",
  "bin": "bin/index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha && standard",
    "start": "npm run nodemon & npm run watch:webpack & wait",
    "prepublish": "npm test && npm run build",
    "uninstall": "node bin/uninstall.js",
    "precommit": "npm test",
    "nodemon": "nodemon -- src/daemon",
    "watch:webpack": "webpack -d --watch",
    "build": "npm run build:webpack && npm run build:babel",
    "build:babel": "babel src -d lib --copy-files --ignore src/daemon/public/bundle.js",
    "build:webpack": "webpack -p --output-path lib/daemon/public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typicode/hotel.git"
  },
  "keywords": [
    "dev",
    "utility",
    "process",
    "manager",
    "local",
    "server"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typicode/hotel/issues"
  },
  "homepage": "https://github.com/typicode/hotel",
  "dependencies": {
    "body-parser": "^1.14.2",
    "chokidar": "^1.2.0",
    "connect-sse": "^1.2.0",
    "debug": "^2.2.0",
    "exit-hook": "^1.1.1",
    "express": "^4.13.3",
    "get-port": "^2.0.0",
    "http-proxy": "^1.12.0",
    "mkdirp": "^0.5.1",
    "once": "^1.3.2",
    "os-homedir": "^1.0.1",
    "respawn-group": "^1.0.1",
    "server-ready": "^0.2.0",
    "strip-ansi": "^3.0.0",
    "sudo-block": "^1.2.0",
    "tildify": "^1.1.2",
    "unquote": "^1.1.0",
    "update-notifier": "^0.5.0",
    "user-startup": "^0.1.1",
    "vhost": "^3.0.2",
    "vue": "^1.0.16",
    "yargs": "^3.29.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.2",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-object-assign": "^6.1.18",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.1.19",
    "babel-preset-es2015": "^6.5.0",
    "babel-runtime": "^5.8.35",
    "css-loader": "^0.23.1",
    "husky": "^0.11.3",
    "mocha": "^2.4.2",
    "mock-fs": "^3.6.0",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.3",
    "standard": "^4.5.4",
    "supertest": "^0.15.0",
    "untildify": "^2.1.0",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.1.0",
    "vue-loader": "^8.1.1",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.13",
    "whatwg-fetch": "^0.11.0"
  }
}
