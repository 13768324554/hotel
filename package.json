{
  "name": "hotel",
  "version": "0.7.6",
  "description": "Local domains for everyone and more! ",
  "main": "lib",
  "bin": "lib/cli/bin.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "ava && npm run lint",
    "lint": "eslint . --ignore-path .gitignore && stylelint './src/front/**/*.css'",
    "fix": "eslint . --ignore-path .gitignore --fix && stylelint './src/front/**/*.css' --fix",
    "start": "run-p start:*",
    "start:parcel": "parcel watch src/front/index.html --public-url /",
    "start:nodemon": "nodemon -- src/daemon",
    "prepublishOnly": "npm run build && pkg-ok",
    "uninstall": "node bin/uninstall.js",
    "precommit": "npm test",
    "build": "run-s build:*",
    "build:parcel": "rimraf dist && parcel build src/front/index.html",
    "build:babel": "rimraf lib && babel src -d lib --copy-files --ignore src/front",
    "addFixtures": "node src/cli/bin add \"node index\" --dir ./test/fixtures/app && node src/cli/bin add \"node index\" --dir ./test/fixtures/verbose && node src/cli/bin add http://some-domain.com --name local-domain "
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typicode/hotel.git"
  },
  "keywords": [
    "dev",
    "utility",
    "process",
    "manager",
    "local",
    "server",
    "host",
    "proxy"
  ],
  "author": "Typicode <typicode@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typicode/hotel/issues"
  },
  "homepage": "https://github.com/typicode/hotel",
  "dependencies": {
    "after-all": "^2.0.2",
    "ansi2html": "0.0.1",
    "chalk": "^2.0.1",
    "chokidar": "^1.2.0",
    "connect-sse": "^1.2.0",
    "exit-hook": "^1.1.1",
    "express": "^4.13.3",
    "get-port": "^3.1.0",
    "http-proxy": "^1.16.2",
    "matcher": "^1.0.0",
    "mkdirp": "^0.5.1",
    "once": "^1.3.2",
    "please-upgrade-node": "^3.0.0",
    "pug": "^2.0.0-beta11",
    "respawn": "^2.4.1",
    "selfsigned": "^1.10.1",
    "server-ready": "^0.3.1",
    "strip-ansi": "^4.0.0",
    "sudo-block": "^2.0.0",
    "tildify": "^1.1.2",
    "tinydate": "^1.0.0",
    "unquote": "^1.1.0",
    "untildify": "^3.0.2",
    "update-notifier": "^2.2.0",
    "user-startup": "^0.2.1",
    "vhost": "^3.0.2",
    "yargs": "^8.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.0.34",
    "@types/react-dom": "^16.0.3",
    "ava": "^0.23.0",
    "babel-cli": "^6.1.2",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.5",
    "corsproxy": "^1.5.0",
    "escape-html": "^1.0.3",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.2",
    "hyperapp": "^0.16.0",
    "json-loader": "^0.5.4",
    "lodash.uniqueid": "^4.0.1",
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "mock-fs": "^4.2.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.8.1",
    "npm-run-all": "^4.0.2",
    "parcel-bundler": "^1.4.1",
    "pkg-ok": "^1.0.1",
    "prettier": "^1.7.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-icons": "^2.2.7",
    "rimraf": "^2.5.2",
    "sinon": "^3.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-recess-order": "^1.2.3",
    "stylelint-config-standard": "^18.0.0",
    "supertest": "^3.0.0",
    "tempy": "^0.2.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.6.2"
  },
  "ava": {
    "serial": true,
    "verbose": true,
    "require": [
      "babel-register",
      "./test/_setup"
    ],
    "babel": "inherit"
  }
}
